.toast_content_box {  
  display: flex;  
  width: 100%;  
  height: 100%;  
  justify-content: center;  
  align-items: center;  
  position: fixed;  
  z-index: 999;  
}  
.toast_content {  
  width: 35%;  
  padding: 20rpx;  
  background: rgba(0, 0, 0, 0.7);  
  border-radius: 20rpx;  
}  
.toast_content_text {  
  height: 100%;  
  width: 100%;  
  color: #fff;  
  font-size: 28rpx;  
  text-align: center;  
  white-space: normal;
  word-break: break-all;
  word-wrap: break-word;
} 
.toast_content_img{
  width: 100%;
  height: 30px;
  margin-bottom: 5px;
}
.toast_content_myimg{
  width: 100%;
  height: 30px;
  text-align: center;
}
.toast_content_myimg image{
  width: 30px;
  height: 30px;
}
.mt-20{
  margin-top: 10px;
}
.success{
  background: url('data:image/png;base64,') no-repeat center center;
  background-size:25px 25px;
}
.error{
  background: url('data:image/png;base64,') no-repeat center center;
  background-size:25px 25px;
}
.warning{
  background: url('data:image/png;base64,') no-repeat center center;
  background-size:25px 25px;
}